void a(Thing thing) {
  pushMatrix();
  float maar = random(40);
  ya.disableStyle();
  //rotate(caer);
  noStroke();
  //translate(random(32), random(32));
  shapeMode(CENTER);
  fill(thing.c, 255);
  shape(ya, 0, 0, maar, maar);  
  popMatrix();
}


